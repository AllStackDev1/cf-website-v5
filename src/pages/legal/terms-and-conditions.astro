---
import Layouts from "../../layouts/Layouts.astro";

import Header from "../../components/legal/Header";

import {
  Intro,
  Payment,
  Conducts,
  CopyRight,
  Liability,
  Assignment,
  AnitBribery,
  UseByMinors,
  Disclaimers,
  Termination,
  GeneralTerms,
  Governinglaw,
  Miscellaneous,
  Modifications,
  PrivacyPolicy,
  Communications,
  ServiceLicense,
  Indemnification,
  ReservationRights,
  DisputeResolution,
  RelationshipParties,
  ServiceRestrictions,
  EligiblityRestriction,
  TerritorialRestriction,
} from "@components/legal/terms-conditions";
---

<Layouts
  title="Terms & Conditions | Complete Farmer"
  description="We are providing cutting-edge technological farming protocols and innovations with a unique business model and logistics that are revolutionizing..."
>
  <Header
    client:visible
    title="Terms & Conditions"
    modifiedTime="Last modified: 28 January 2022"
  />

  <div class="bg-white py-8 sm:py-14 sm:px-6">
    <div class="mx-auto max-w-7xl px-6 lg:px-8">
      <GeneralTerms client:visible />
      <Intro client:visible />
      <TerritorialRestriction client:visible />
      <UseByMinors client:visible />
      <PrivacyPolicy client:visible />
      <EligiblityRestriction client:visible />
      <Payment client:visible />
      <ServiceLicense client:visible />
      <ServiceRestrictions client:visible />
      <ReservationRights client:visible />
      <Conducts client:visible />
      <Communications client:visible />
      <Indemnification client:visible />
      <Disclaimers client:visible />
      <Liability client:visible />
      <Termination client:visible />
      <CopyRight client:visible />
      <Assignment client:visible />
      <AnitBribery client:visible />
      <Modifications client:visible />
      <RelationshipParties client:visible />
      <Governinglaw client:visible />
      <DisputeResolution client:visible />
      <Miscellaneous client:visible />
    </div>
  </div>
</Layouts>

<script type="text/javascript">
  window.dataLayer.push({
    event: "pageview",
    page: "/legal/terms-and-conditions",
  });

  // ga("set", "page", "/contact-us");
  // ga("send", "pageview");

  window.fbq("track", "PageView");

  // https://www.thyngster.com/google-tag-manager-google-analytics-4-ga4-events-setup-with-a-single-tag
  // https://www.thyngster.com/tracking-google-analytics-4-events-using-data-attributes
  // https://developers.facebook.com/docs/meta-pixel/advanced/
  // https://developers.facebook.com/docs/meta-pixel/implementation/conversion-tracking
</script>
